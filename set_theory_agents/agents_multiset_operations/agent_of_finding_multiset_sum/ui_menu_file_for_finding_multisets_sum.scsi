ui_menu_file_for_finding_multisets_sum 
	<- 	ui_user_command_class_atom; 
		ui_user_command_class_view_kb;
	=> nrel_main_idtf: 
		[Какова арифметическая сумма данных мультимножеств?]
			(* <- lang_ru;; *);
		[What is the result of arithmetic sum of given multisets?]
			(* <- lang_en;; *);
	=> ui_nrel_command_template:
		[*
		question_finding_multisets_sum _-> ._question_finding_multisets_sum_instance
			(*
				_-> rrel_1:: ui_arg_1;;
				_-> rrel_2:: ui_arg_2;;
			*);;
		._question_finding_multisets_sum_instance _<- question;;
		*];
	=> ui_nrel_command_lang_template: 
		[Запрос нахождения арифметической суммы мультимножеств: $ui_arg_1 и $ui_arg_2] 
			(* <- lang_ru;; *);
		[Request to find the arithmetic sum of multisets: $ui_arg_1 и $ui_arg_2]
			(* <- lang_en;; *);;
